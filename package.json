{
  "name": "video-downloader-render",
  "version": "2.0.0",
  "description": "Multi-platform video downloader API optimized for Render.com deployment",
  "main": "server-render.js",
  "scripts": {
    "start": "node server-render.js",
    "dev": "nodemon server-render.js",
    "render": "node server-render.js",
    "test": "node -e \"console.log('Server test passed')\"",
    "test-cookies": "node test-cookies.js",
    "postinstall": "echo 'Dependencies installed successfully for Render.com'"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@distube/ytdl-core": "^4.13.5",
    "axios": "^1.4.0",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^6.11.2",
    "node-fetch": "^2.6.12",
    "yt-dlp-exec": "^1.0.2",
    "ytdl-core": "^4.11.5"
  },
  "devDependencies": {
    "nodemon": "^2.0.22"
  },
  "keywords": [
    "video",
    "downloader",
    "youtube",
    "twitter",
    "tiktok",
    "instagram",
    "facebook",
    "vimeo",
    "pinterest",
    "yt-dlp",
    "render",
    "api"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/video-downloader.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/video-downloader/issues"
  },
  "homepage": "https://github.com/yourusername/video-downloader#readme",
  "render": {
    "buildCommand": "npm install",
    "startCommand": "npm start",
    "healthCheckPath": "/health",
    "envVars": {
      "NODE_ENV": "production",
      "RENDER": "true"
    }
  }
}
